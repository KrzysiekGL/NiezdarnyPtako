<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement name="root" style="height: 100%; width: 100%; justify-content: center; flex-direction: column; align-items: center; padding-left: 25px; padding-right: 25px; padding-top: 25px; padding-bottom: 25px;">
        <ui:VisualElement style="width: 100%; height: 20%; padding-left: 5%; padding-right: 5%; padding-top: 5%; padding-bottom: 5%;" />
        <ui:VisualElement style="height: 30%; width: 30%; justify-content: center; align-items: center;">
            <ui:Label tabindex="-1" text="Press SPACE to play" display-tooltip-when-elided="true" name="GameStartInfobox" style="align-items: center; font-size: 100px; -unity-text-align: middle-center;" />
            <ui:Label tabindex="-1" text="Press C to change the color of Ptako" display-tooltip-when-elided="true" name="ChangeColorInfobox" style="font-size: 64px; justify-content: center; align-items: center; -unity-text-align: upper-center; color: rgb(60, 60, 60);" />
            <ui:Label tabindex="-1" text="Press ESC to quit the game" display-tooltip-when-elided="true" name="quit" style="font-size: 30px; -unity-text-align: middle-center; padding-top: 25%; padding-left: 25%; padding-right: 25%; padding-bottom: 25%; margin-top: 0; margin-left: 0; margin-right: 0; margin-bottom: 0; height: auto; white-space: nowrap;" />
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
